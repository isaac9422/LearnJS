#include<iostream>
#include<cstdio>
#include<cmath>
 
using namespace std;
 
int aux[10000];
int v[10]={7,3,14,42,3,11,32,20,10,2};
 
void Merge(int lo,int mid, int hi)
{
    int k=0,m = hi-lo+1;
    int i=lo,j=mid+1;
    while(k<m)
    {
        if((i<=mid && j<=hi && v[i]<=v[j] ) || j>hi )
            aux[k++] = v[i++];
        else if(j<=hi || i>mid)
            aux[k++] = v[j++];
    }
    for(int i=0;i<m;i++)
        v[lo++] = aux[i];
}
 
void MergeSort(int lo,int hi)
{
    if(lo>=hi) return;
    int mid = (hi+lo)/2;
    MergeSort(lo,mid);
    MergeSort(mid+1,hi);
    Merge(lo,mid,hi);
}
int main()
{
    MergeSort(0,9);
    for(int i=0;i<10;cout<<v[i++]<<endl);
    return 0;
}
 
